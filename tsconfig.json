{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "rootDir": "./",
    "baseUrl": "./",
    "paths": {
      "*": ["src/*"],
      "@Components/*": ["src/components/*"],
      "@Application/*": ["src/application/*"],
      "@Domain/*": ["src/domain/*"],
      "@Enums/*": ["src/enums/*"],
      "@Interfaces/*": ["src/interfaces/*"],
      "@Lib/*": ["src/lib/*"],
      "@Navigation/*": ["src/navigation/*"],
      "@Services/*": ["src/services/*"],
      "@Types/*": ["src/types/*"],
      "@Views/*": ["src/views/*"]
    }
  }
}
